{
  "name": "<%= applicationName %>",
  "version": "0.1.0",
  "description": "<%= applicationDesc %>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/server/server.js",
    "lint": "next lint",
    "pm2:start": "pm2 start 'npm start'",
    "pm2:stop": "pm2 stop 'all'",
    "pm2:delete": "pm2 delete 'all'",
    "postinstall": "husky install"
  },
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@koa/router": "^11.0.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/material-nextjs": "^5.15.7",
    "axios": "^0.27.2",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "koa": "^2.13.4",
    "log4js": "^6.5.1",
    "mobx": "^6.3.9",
    "mobx-react-lite": "^4.0.5",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/koa__router": "^8.0.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.3",
    "pm2": "^5.3.1",
    "prettier": "^3.2.4",
    "sass": "^1.42.1",
    "stylelint": "^16.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./{pages,src,server,lib,utils}/**/*.{js,jsx,ts,tsx,json}": [
      "tsc --noEmit",
      "npx prettier --write",
      "eslint './{pages,src,server,lib,utils}/**/*.{js,jsx,ts,tsx,json}' --fix"
    ],
    "./{pages,src,utils}/**/*.{scss,sass,css}": [
      "npx prettier --write",
      "stylelint './{pages,src,utils}/**/*.{scss,sass,css}' --fix"
    ]
  }
}
