    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "postcss-preset-env": "^10.0.5",
    "prettier": "^3.3.3",
    "terser": "^5.33.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.7"
  },
  "dependencies": {
    "antd": "^5.21.0",
    "axios": "^1.7.7",
