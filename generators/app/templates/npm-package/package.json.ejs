{
  "name": "<%= packageName %>",
  "version": "0.1.0",
  "description": "<%= applicationDesc %>",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --extensions '.ts,.tsx'",
    "build:es": "babel src --out-dir es --extensions '.ts,.tsx'",
    "build:umd": "rollup -c",
    "build": "npm run build:commonjs && npm run build:es && npm run types",
    "types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib/* dist/* es/* types/*",
    "prepublish": "npm run clean && npm run build",
    "postinstall": "husky install"
  },
  "keywords": [
    "react",
    "templates"
  ],
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-external-helpers": "^7.22.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "core-js": "^3.31.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint './src/**/*.{js,jsx,ts,tsx}' --fix"
    ]
  }
}
