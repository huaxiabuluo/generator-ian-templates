{
  "name": "<%= packageName %>",
  "version": "0.1.0",
  "description": "<%= applicationDesc %>",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --extensions '.ts,.tsx'",
    "build:es": "babel src --out-dir es --extensions '.ts,.tsx'",
    "build:umd": "rollup -c",
    "build": "npm run build:commonjs && npm run build:es && npm run types",
    "types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib/* dist/* es/* types/*",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "templates"
  ],
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.6.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/js-cookie": "^2.2.6",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.55",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "core-js": "^3.4.7",
    "cross-env": "^6.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.5.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.33.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^16.8.3 || ^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint './src/**/*.{js,jsx,ts,tsx}' --fix"
    ]
  }
}
